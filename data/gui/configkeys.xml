            <widget factory="pawsSeqTreeNode" name="Brightness down"/>
            <widget factory="pawsSeqTreeNode" name="Brightness Reset" />
        </widget>
        <widget factory="pawsWidgetTreeNode" name="Shortcuts" >
            <nodewidget>
                <widget factory="pawsTextBox" name="Shortcuts Label">
                    <frame x="0" y="4" width="100" height="20" border="no" />
