<widget_description>

  <widget name="ConfigKeysTree" factory="pawsConfigKeys"
          visible="yes" savepositions="no" movable="no" resizable="no" style="New Standard GUI">
    <bgimage resource="Scaling Widget Background" />

    <widget factory="pawsTree">
      <layout rowSpacing="5" levelSpacing="20" />

      <widget factory="pawsSeqTreeNode">

        <widget factory="pawsWidgetTreeNode" name="Movement">
            <nodewidget>
                <widget factory="pawsTextBox" name="Movement label">
                    <frame x="0" y="4" width="100" height="20" border="no" />
                    <text string="Movements"/>
                </widget>
            </nodewidget>
            <widget factory="pawsSeqTreeNode" name="MouseMove"/>
            <widget factory="pawsSeqTreeNode" name="Forward"/>
            <widget factory="pawsSeqTreeNode" name="Backward"/>
            <widget factory="pawsSeqTreeNode" name="Rotate left"/>
            <widget factory="pawsSeqTreeNode" name="Rotate right"/>
            <widget factory="pawsSeqTreeNode" name="Strafe left"/>
            <widget factory="pawsSeqTreeNode" name="Strafe right"/>
            <widget factory="pawsWidgetTreeNode" name="Alternates" collapsed="yes">
                <nodewidget>
                    <widget factory="pawsTextBox" name="Alternates label">
                        <frame x="0" y="4" width="100" height="20" border="no" />
                        <text string="Alternates"/>
                    </widget>
                </nodewidget>
                <widget factory="pawsSeqTreeNode" name="Forward (sec)"/>
                <widget factory="pawsSeqTreeNode" name="Backward (sec)"/>
                <widget factory="pawsSeqTreeNode" name="Rotate left (sec)"/>
                <widget factory="pawsSeqTreeNode" name="Rotate right (sec)"/>
                <widget factory="pawsSeqTreeNode" name="Strafe left (sec)"/>
                <widget factory="pawsSeqTreeNode" name="Strafe right (sec)"/>
            </widget>
        </widget>
        <widget factory="pawsWidgetTreeNode" name="Modifiers">
            <nodewidget>
                <widget factory="pawsTextBox" name="Modifiers label">
                    <frame x="0" y="4" width="100" height="20" border="no" />
                    <text string="Modifiers"/>
                </widget>
            </nodewidget>
            <widget factory="pawsSeqTreeNode" name="Run"/>
            <widget factory="pawsSeqTreeNode" name="ToggleRun"/>
            <widget factory="pawsSeqTreeNode" name="AutoMove"/>
            <widget factory="pawsSeqTreeNode" name="Sneak"/>
        </widget>
        <widget factory="pawsWidgetTreeNode" name="Actions">
            <nodewidget>
                <widget factory="pawsTextBox" name="Actions label">
                    <frame x="0" y="4" width="100" height="20" border="no" />
                    <text string="Actions"/>
                </widget>
            </nodewidget>
            <widget factory="pawsSeqTreeNode" name="Jump"/>
            <widget factory="pawsSeqTreeNode" name="Sit"/>
        </widget>
        <widget factory="pawsWidgetTreeNode" name="Camera">
            <nodewidget>
                <widget factory="pawsTextBox" name="Camera label">
                    <frame x="0" y="4" width="100" height="20" border="no" />
                    <text string="Camera"/>
                </widget>
            </nodewidget>
            <widget factory="pawsSeqTreeNode" name="Camera mode"/>
            <widget factory="pawsSeqTreeNode" name="Look up"/>
            <widget factory="pawsSeqTreeNode" name="Look down"/>
            <widget factory="pawsSeqTreeNode" name="Zoom in"/>
            <widget factory="pawsSeqTreeNode" name="Zoom out"/>
            <widget factory="pawsSeqTreeNode" name="Center camera"/>
            <widget factory="pawsSeqTreeNode" name="MouseLook"/>
            <widget factory="pawsSeqTreeNode" name="Toggle MouseLook"/>
            <widget factory="pawsSeqTreeNode" name="CameraForward/Back"/>
        </widget>
        <widget factory="pawsWidgetTreeNode" name="Game Control">
            <nodewidget>
                <widget factory="pawsTextBox" name="Game Control label">
                    <frame x="0" y="4" width="100" height="20" border="no" />
