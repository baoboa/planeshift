            <widget name="badwordsincoming" factory="pawsCheckBox">
                <frame x="10" y="280" width="180" height="20" />
                <text string="Filter incoming bad words" />
            </widget>

            <widget name="badwordsoutgoing" factory="pawsCheckBox">
                <frame x="200" y="280" width="180" height="20" />
                <text string="Filter outgoing bad words" />
            </widget>

            <widget name="selecttabstyle" factory="pawsRadioButtonGroup">
                <frame x="255" y="0" width="150" height="120" />
                <widget factory="pawsTextBox" name="">
                    <frame x="10" y="0" width="180" height="20" border="no" />
                    <text string="Auto Tab selection:"/>
                </widget>
                <radionode name="none" id="0" >
                    <frame x="10" y="20" width="180" height="20" />
                    <text string="Don't autoselect" position="right" />
                </radionode>
                <radionode name="fitting" id="1" >
                    <frame x="10" y="50" width="180" height="20" />
                    <text string="Select fitting Tab" position="right" />
                </radionode>
                <radionode name="alltext" id="2" >
                    <frame x="10" y="80" width="180" height="20" />
                    <text string="Select Main Tab" position="right" />
                </radionode>
            </widget>

            <widget name="joindefaultchannel" factory="pawsCheckBox">
                <frame x="255" y="120" width="160" height="20" />
                <text string="Join default channel"/>
            </widget>

            <widget name="echoscreeninsystem" factory="pawsCheckBox">
                <frame x="255" y="140" width="160" height="20" />
                <text string="Echo screen text" />
            </widget>

            <widget name="mainbrackets" factory="pawsCheckBox">
                <frame x="255" y="160" width="160" height="20" />
                <text string="Main chat brackets" />
            </widget>

            <widget name="yourcolormix" factory="pawsCheckBox">
                <frame x="255" y="180" width="160" height="20" />
                <text string="Mix yourtext color" />
            </widget>

            <widget name="loosefocus" factory="pawsCheckBox">
                <frame x="255" y="200" width="160" height="20"/>
                <text string="Lose focus on send" />
            </widget>

            <widget name="mousefocus" factory="pawsCheckBox">
                <frame x="255" y="220" width="160" height="20"/>
                <text string="Focus on mouse over" />
            </widget>

            <widget name="defaultlastchat" factory="pawsCheckBox">
                <frame x="255" y="240" width="160" height="20"/>
                <text string="Default last chat" />
            </widget>

</widget>

</widget_description>

