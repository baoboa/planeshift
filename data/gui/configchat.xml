    <bgimage resource="Scaling Widget Background" />

            <widget name="systemtext" factory="pawsTextBox">
                <frame x="10" y="0" width="130" height="20" />
                <text string="System text color" />
            </widget>
            <widget name="systemtextr" factory="pawsEditTextBox"><frame border="yes" x="140" y="0" width="30" height="20" /></widget>
            <widget name="systemtextg" factory="pawsEditTextBox"><frame border="yes" x="180" y="0" width="30" height="20" /></widget>
            <widget name="systemtextb" factory="pawsEditTextBox"><frame border="yes" x="220" y="0" width="30" height="20" /></widget>

