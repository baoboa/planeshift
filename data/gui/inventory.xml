                    <frame x="0" y="0" width="48" height="48" />
                    <subscriptions>
                        <subscribe data="sigClearSlot" />
                    </subscriptions>
                </widget>
            </column>
            <column width="48" >
                <widget name="Bulk4" factory="pawsSlot">
                    <frame x="0" y="0" width="48" height="48" />
