<widget_description>

<widget name="FingeringWindow" factory="pawsFingeringWindow" visible="no" savepositions="no" movable="yes" resizable="no" style="New Standard GUI">

    <frame x="300" y="200" width="450" height="100" border="yes" />

    <widget name="LabelTextBox" factory="pawsTextBox">
        <frame x="15" y="15" width="420" height="30" border="no" />
    </widget>

    <widget name="ButtonTextBox" factory="pawsTextBox">
        <frame x="15" y="45" width="420" height="30" border="no" />
    </widget>

    <widget name="SetButton" factory="pawsButton" id="-30" toggle="no" style="Standard Button">
        <frame x="90" y="70" width="60" height="20" border="no"/>
        <label text="Set"/>
    </widget>

    <widget name="NoneButton" factory="pawsButton" id="-20" toggle="no" style="Standard Button">
        <frame x="160" y="70" width="60" height="20" border="no"/>
        <label text="None"/>
    </widget>

    <widget name="CancelButton" factory="pawsButton" id="-10" toggle="no" style="Standard Button">
        <frame x="230" y="70" width="60" height="20" border="no"/>
        <label text="Cancel"/>
    </widget>

</widget>

</widget_description>
