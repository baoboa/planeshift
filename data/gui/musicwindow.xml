<widget_description>
    <widget name="MusicWindow" factory="pawsMusicWindow" visible="no" movable="yes" resizable="no" keepaspect="no">
        <frame x="100" y="200" width="600" height="325" border="Leather Sketch Border" />
        <minframe height="20" />
        <title resource="MiddleAreaMap" text="Song Title" align="left" close_button="yes" shadow="false" />
        <bgimage resource="MiddleAreaMap" />
        <font name="scurlock.ttf" r="0" g="0" b="0" size="20" />

        <widget name="EditButton" factory="pawsButton" id="100" toggle="yes" tooltip="Edit Mode">
            <frame x="0" y="0" width="16" height="16" />
            <buttonup resource="EditButtonOff" />
            <buttondown resource="EditButtonOn" />
            <eventMouseDown>Ret = MusicWindow:ClickEditButton()</eventMouseDown>
        </widget>

        <widget name="PlayButton" factory="pawsButton" id="101" toggle="yes" tooltip="Play/Stop">
            <frame x="18" y="0" width="16" height="16" />
            <buttonup resource="PlayButton" />
            <buttondown resource="StopButton" />
            <eventMouseDown>Ret = MusicWindow:ClickPlayButton()</eventMouseDown>
        </widget>

        <widget name="LoadButton" factory="pawsButton" id="102" tooltip="Load">
            <frame x="36" y="0" width="16" height="16" />
