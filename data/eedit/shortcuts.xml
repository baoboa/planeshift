<widget_description>



<widget name="ShortcutsToolbox" factory="EEditShortcutsToolbox" 

        visible="no" savepositions="yes" movable="yes" resizable="no" resizetoscreen="no" >

        

    <frame x="0" y="0" width="420" height="240" border="yes" justtitle="yes" />

    <title resource="TitleBar" text="EEdit Shortcuts" align="left" />

    <font name="LiberationSans-Regular.ttf" r="255" g="255" b="255" size="9"  sr="0" sg="0" sb="0" shadow="yes" />   

    <bgimage resource="Back" alpha="75" />



    <widget name="shortcuts_list" factory="pawsListBox">	

        <frame x="10" y="10" width="400" height="190" border="no"/>

        <bgimage resource="Shaded" alpha="100" />

        <font name="LiberationSans-Regular.ttf" r="255" g="255" b="255" size="9"  sr="0" sg="0" sb="0" shadow="yes" />   

        <highlight resource="ListSelect" alpha="100" />

        <columns height="20" headings="no">

            <column width="200">            

                <widget name="Command" factory="pawsTextBox">

                </widget>

            </column>

            <column width="180">

                <widget name="Key" factory="pawsKeySelectBox">

                </widget>

            </column>

        </columns>

	</widget>



    <widget name="apply" factory="pawsButton">

        <frame x="190" y="205" width="70" height="25" border="no" />

        <font name="LiberationSans-Regular.ttf" r="255" g="255" b="255" size="7"  sr="0" sg="0" sb="0" shadow="yes" />   

        <buttonup resource="ButtonUp"/>

        <buttondown resource="ButtonDown" />

        <label text="Apply"/>

    </widget>

    <widget name="cancel" factory="pawsButton">

        <frame x="265" y="205" width="70" height="25" border="no" />

        <font name="LiberationSans-Regular.ttf" r="255" g="255" b="255" size="7"  sr="0" sg="0" sb="0" shadow="yes" />   

        <buttonup resource="ButtonUp"/>

        <buttondown resource="ButtonDown" />

        <label text="Cancel"/>

    </widget>

    <widget name="ok" factory="pawsButton">

        <frame x="340" y="205" width="70" height="25" border="no" />

        <font name="LiberationSans-Regular.ttf" r="255" g="255" b="255" size="7"  sr="0" sg="0" sb="0" shadow="yes" />   

        <buttonup resource="ButtonUp"/>

        <buttondown resource="ButtonDown" />

        <label text="Ok"/>

    </widget>

</widget>



</widget_description>

