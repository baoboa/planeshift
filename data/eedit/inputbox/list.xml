<widget_description>

<widget name="SelectList" factory="EEditSelectList" 
        visible="yes" savepositions="no" movable="yes" resizable="no" resizetoscreen="no" >
        
    <frame x="0" y="0" width="163" height="200" border="yes" justtitle="yes" />
    <title resource="TitleBar" text="Select" align="left" />
    <font name="LiberationSans-Regular.ttf" r="255" g="255" b="255" size="9"  sr="0" sg="0" sb="0" shadow="yes" />   
    <bgimage resource="Back" alpha="0" />

    <widget name="select_value" factory="pawsListBox">
        <frame x="3" y="3" width="157" height="164" border="no" />        
        <font name="LiberationSans-Regular.ttf" r="255" g="255" b="255" size="9"  sr="0" sg="0" sb="0" shadow="yes" />   
        <bgimage resource="Shaded" alpha="75" />
        <highlight resource="ListSelect" alpha="100" />
        <columns height="20" headings="no">
           <column width="137">            
              <widget name="ListItem" factory="pawsTextBox">
              </widget>
           </column>                                               
       </columns>
    </widget>
    <widget name="ok" factory="pawsButton">
        <frame x="115" y="170" width="45" height="25" border="no" />
        <font name="LiberationSans-Regular.ttf" r="255" g="255" b="255" size="7"  sr="0" sg="0" sb="0" shadow="yes" />   
        <buttonup resource="ButtonUp"/>
        <buttondown resource="ButtonDown" />
        <label text="Ok"/>
    </widget>
</widget>

</widget_description>
