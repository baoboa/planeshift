<widget_description>

<widget name="EditEffectToolbox" factory="EEditEditEffectToolbox" 
        visible="yes" savepositions="yes" movable="yes" 
        resizable="no" resizetoscreen="no" >
        
    <frame x="8" y="30" width="310" height="690" border="yes" justtitle="yes" />
    <title resource="TitleBar" text="Effect" align="left" />
    <font name="LiberationSans-Regular.ttf" r="255" g="255" b="255" size="9"  sr="0" sg="0" sb="0" shadow="yes" />   
    <bgimage resource="BackTall" alpha="75" />

    <widget name="EffectItemsTree" factory="pawsTree">
        
        <frame x="5" y="5" width="300" height="645" border="no" />

        <decorator selR="0" selG="0" selB="0" lineR="153" lineG="153" lineB="153" />
        
        <widget factory="pawsSimpleTreeNode" name="EffectItemsRoot" />

    </widget>
    <widget name="ApplyEffect" factory="pawsButton" toggle="no">
        <frame x="200" y="655" width="90" height="25" border="no" />
        <font name="LiberationSans-Regular.ttf" r="255" g="255" b="255" size="7"  sr="0" sg="0" sb="0" shadow="yes" />   
        <buttonup resource="ButtonUp"/>
        <buttondown resource="ButtonDown" />
        <label text="Save Effect"/>
    </widget>
    
</widget>

</widget_description>
