<?xml version="1.0" encoding="utf-8" ?> 
<library>
    <shaders>
        <shader>
            <file>/shader/lighting/fullbright.xml</file>
        </shader>
    </shaders>
    <textures>
        <texture name="marker">
            <file>/this/art/effects/marker02.dds</file>
        </texture>
    </textures>
    <materials>
        <material name="marker">
            <texture>marker</texture>
	      <shader type="base">lighting_fullbright</shader>
