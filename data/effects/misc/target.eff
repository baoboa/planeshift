                    <action name="scale" value="0.8" />
                    <action name="rotate" x="0" y="0" z="0" />
                    <action name="spin" y="-90" />
                    <action name="colour" r="200" g="0" b="40" />
                </keyFrame>
                <keyFrame time="2500">
